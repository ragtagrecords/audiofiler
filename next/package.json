{
  "name": "@examples/trpc-next-prisma-starter",
  "version": "10.7.0",
  "private": true,
  "scripts": {
    "migrate-mysql": "prisma migrate dev --name init",
    "generate-mysql": "prisma generate",
    "prisma-studio-sqlite": "prisma studio --schema=./prisma/_sqlite/schema.prisma",
    "prisma-studio-postgres": "prisma studio",
    "db-seed": "prisma db seed",
    "db-reset": "prisma migrate reset",
    "dx:next": "run-s migrate-mysql generate-mysql db-seed && next dev",
    "dx:prisma-studio": "pnpm prisma-studio-sqlite",
    "dx": "run-p dx:* --print-label",
    "dev": "npm run dx:next",
    "prep-sqlite": "run-s generate-sqlite migrate-sqlite",
    "build-sqlite": "next build -p 3003",
    "build-postgres": "run-s generate-postgres migrate && next build",
    "build": "pnpm build-postgres",
    "start": "next start -p 3003",
    "lint": "eslint --ext \".js,.ts,.tsx\" --report-unused-disable-directives --report-unused-disable-directives src",
    "lint-fix": "pnpm lint --fix",
    "migrate-dev": "prisma migrate dev",
    "migrate": "prisma migrate deploy",
    "test": "run-s test:*",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test-dev": "start-server-and-test 'next dev' http://127.0.0.1:3003 test",
    "test-start": "start-server-and-test start http://127.0.0.1:3003 test",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {
    "@prisma/client": "^4.7.1",
    "@tanstack/react-query": "^4.3.8",
    "@trpc/client": "^10.7.0",
    "@trpc/next": "^10.7.0",
    "@trpc/react-query": "^10.7.0",
    "@trpc/server": "^10.7.0",
    "clsx": "^1.1.1",
    "next": "^13.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "superjson": "^1.7.4",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.26.1",
    "@types/node": "^18.7.20",
    "@types/react": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "eslint": "^8.30.0",
    "eslint-config-next": "^13.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prisma": "^4.7.1",
    "start-server-and-test": "^1.12.0",
    "tsx": "^3.9.0",
    "typescript": "^4.8.3",
    "vite": "^3.1.3",
    "vitest": "^0.23.4"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
